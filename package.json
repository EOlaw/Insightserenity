{
  "name": "insightserenity-platform",
  "version": "0.1.0",
  "private": true,
  "author": "InsightSerenity Team",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "scripts": {
    "dev": "next dev --turbopack",
    "dev:server": "nodemon server.js",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write .",
    "clean": "rimraf .next && rimraf node_modules && rimraf package-lock.json && rimraf dist",
    "clean:cache": "rimraf .next/cache && rimraf node_modules/.cache",
    "clean:all": "rimraf .next && rimraf node_modules && rimraf package-lock.json && rimraf dist && rimraf .cache",
    "prettier": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,md,html}\"",
    "prettier:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,css,md,html}\"",
    "prettier:fix": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,md,html}\"",
    "eslint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "eslint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "eslint:check": "eslint . --ext .js,.jsx,.ts,.tsx --no-fix",
    "test": "jest --watchAll",
    "test:ci": "jest --ci --coverage",
    "test:watch": "jest --watch",
    "test:debug": "jest --runInBand --no-cache --detectOpenHandles",
    "test:coverage": "jest --coverage",
    "test:coverage:html": "jest --coverage --coverageReporters=html",
    "test:coverage:json": "jest --coverage --coverageReporters=json",
    "test:coverage:lcov": "jest --coverage --coverageReporters=lcov",
    "test:coverage:text": "jest --coverage --coverageReporters=text",
    "test:coverage:clover": "jest --coverage --coverageReporters=clover",
    "test:coverage:teamcity": "jest --coverage --coverageReporters=teamcity",
    "test:coverage:json-summary": "jest --coverage --coverageReporters=json-summary",
    "test:coverage:json-lcov": "jest --coverage --coverageReporters=json-lcov",
    "test:coverage:html-spa": "jest --coverage --coverageReporters=html-spa",
    "test:coverage:html-app": "jest --coverage --coverageReporters=html-app",
    "test:coverage:html-standalone": "jest --coverage --coverageReporters=html-standalone",
    "test:coverage:html-standalone-spa": "jest --coverage --coverageReporters=html-standalone-spa",
    "test:coverage:html-standalone-app": "jest --coverage --coverageReporters=html-standalone-app"
  },
